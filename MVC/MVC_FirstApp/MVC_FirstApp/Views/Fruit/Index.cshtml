
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Fruit List</title>
</head>
<body>
    <div> 
        <h3>Fruit List</h3>

        @{ 
            List<string> fruitList = Session["Fruits"] as List<string>;
        }

        <ul>
            @foreach (var fruit in fruitList)
            {
                <li>
                    @fruit
                </li>
            }
        </ul>

    </div>
</body>
</html>
